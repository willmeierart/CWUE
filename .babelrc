{
	"env": {
		"development": {
			"exclude": [],
			"plugins": [
				[
					"module-resolver",
					{
						"root": [ "." ],
						"alias": {
							"styles": "./styles"
						},
						"cwd": "babelrc"
					}
				],
				[
					"wrap-in-js",
					{
						"extensions": [ "css$", "scss$" ]
					}
				]
			],
			"presets": [ "next/babel" ]
		},
		"production": {
			"exclude": [],
			"plugins": [
				[
					"module-resolver",
					{
						"root": [ "." ],
						"alias": {
							"styles": "./styles"
						},
						"cwd": "babelrc"
					}
				],
				[
					"wrap-in-js",
					{
						"extensions": [ "css$", "scss$" ]
					}
				]
			],
			"presets": [ "next/babel" ]
		}
	}
}
